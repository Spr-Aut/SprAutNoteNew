<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.spraut.sprautnote">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/app_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SprAutNote">
        <activity android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AddEdit.AddActivity"></activity>
        <activity android:name=".AddEdit.EditActivity"></activity>
        <activity android:name=".AddEdit.BatchAddActivity"></activity>
        <activity android:name=".Image.ImageActivity"></activity>
        <activity android:name=".SearchActivity"></activity>
        <activity android:name="com.lling.photopicker.PhotoPickerActivity"></activity>
    </application>

</manifest>